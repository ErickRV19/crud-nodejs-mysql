<% include partials/_header %>

<div class="container mt-5">
  <div class="row">
    <div class="col-md-5 offset-md-3 card">
      <div class="card-body">
        <form action="/update/<%= data.idProveedores %>" method="POST">
          
          <div class="form-group">
            <input type="text" name="nombre_empresa" value="<%= data.nombre_empresa%>" placeholder="Address" class="form-control">
          </div>
          <div class="form-group">
            <input type="text" name="telefono" value="<%= data.telefono%>" placeholder="Phone" class="form-control">
          </div>
          <div class="form-group">
              <input type="text" name="idDomicilio" value="<%= data.idDomicilio%>" placeholder="Phone" class="form-control">
            </div>
            <div class="form-group">
                <input type="text" name="idProductos" value="<%= data.idProductos%>" placeholder="Phone" class="form-control">
              </div>
              <div class="form-group">
                  <input type="text" name="rfc" value="<%= data.rfc%>" placeholder="Phone" class="form-control">
                </div>
  

          <button type="submit" class="btn btn-primary">
            save customer
          </button>
        </form>
      </div>
    </div>
  </div>
</div>

<% include partials/_footer %>
